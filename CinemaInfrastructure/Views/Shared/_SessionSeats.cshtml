@model IEnumerable<CinemaInfrastructure.ViewModels.SeatViewModel>

@{
    var seatCount = Model.Count();
    string layoutName = seatCount switch
    {
        24 => "_Layout24",
        36 => "_Layout36",
        43 => "_Layout43",
        _ => "_LayoutDefault"
    };
}

@await Html.PartialAsync(layoutName, Model)